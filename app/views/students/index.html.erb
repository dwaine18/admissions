<h1>Student List</h1>

<p></p>

<table>
    <thead>
        <tr>
            <th>Student I.D</th>
            <th>Student First Name</th>
            <th>Student Last Name</th>
            <th>Student SAT Score</th>
        </tr>
    </thead>
    <tbody>
        <%@students.each do |student|%>
        <tr>
            <td><%= link_to student.id, "/students/#{student.id}" %></td> 
            <td><%=student.first_name%></td>
            <td><%=student.last_name%></td>
            <td><%=student.sat_score%></td>
        </tr>
        <%end%>
    </tbody>
</table>
<p></p>

<%=link_to "Create a New  Student", "/students/new"%>